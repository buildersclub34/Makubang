{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "jsx": "react-native",
    "strict": false,
    "noErrorTruncation": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "types": [
      "react",
      "react-native",
      "expo"
    ]
  },
  "exclude": [
    "node_modules",
    "**/node_modules/**"
  ]
}
